<%= form_with url: "/restaurants", method: :post, local: true do |form| %>
  <%= form.label :name %>
  <%= form.text_field :name %><br>
  <%= form.label :yelp_rating%>
  <%= form.number_field :yelp_rating %><br>
  <%= form.label :delivery, 'Restaurant delivers?:' %>
  <%= form.check_box :delivery, {}, true, false %><br>
  <%= form.submit %>
<% end %>
